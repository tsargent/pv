<h1>Listing foos</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @foos.each do |foo| %>
      <tr>
        <td><%= foo.title %></td>
        <td><%= link_to 'Show', [:admin, foo] %></td>
        <td><%= link_to 'Edit', edit_admin_foo_path(foo) %></td>
        <td><%= link_to 'Destroy', [:admin, foo], method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Foo', new_admin_foo_path %>
